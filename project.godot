[application]

config/name="Idle Eden"
run/main_scene="res://MainNavigation/MainMenu/MainMenu.tscn"
config/icon="res://SPRITES/zMisc/icon.png"

[autoload]

Controller="*res://Global/Controller.gd"
SavingManager="*res://Global/SavingManager.gd"
LoadedData="*res://Global/LoadedData.gd"
LastOpenedFiles="*res://addons/file-editor/scripts/LastOpenedFiles.gd"

[display]

window/size/width=1280
window/size/height=720
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/CodeSnippetPopup/plugin.cfg", "res://addons/export_categories/plugin.cfg", "res://addons/file-editor/plugin.cfg", "res://addons/notes_tab/plugin.cfg", "res://addons/project_map/plugin.cfg" )

[global]

window=false

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
